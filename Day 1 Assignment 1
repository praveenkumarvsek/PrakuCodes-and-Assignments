package week1.day1;

public class Fibbinocci {
	public static void main(String[] args) {

	    int n = 11;
	    int firstNum = 0;
	    int secondNum = 1;
	    System.out.println("Fibonacci Series till 11 :");

	    for (int i = 1; i <= n; i = i+1) {
	      System.out.print(firstNum + " ");
	      int sum = firstNum + secondNum;
	      firstNum = secondNum;
	      secondNum = sum;
		
		
		}
	}

}
